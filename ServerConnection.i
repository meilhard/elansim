bool ServerConnection::_isIP()
{
	bool result = true;
	const char* sep = ".";
	char* token;
	char* adr = new char[qstrlen(_adress)];

	qstrcpy(adr, _adress);

	token = strtok(adr, sep);
	
	while ( token && result ) {
		
		int i = 0;
		if ( qstrlen(token) > 3 ) {
			result = false; 
		} else {

			while (token[i]) {
				if ( isalpha(token[i]) ) {
					result = false;
				}
				i++;
			}
		}

		token = strtok(0, sep);
	}

	return result;
}

