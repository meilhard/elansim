int ServerConnection::send(void* msg,int size, Flag flag)
{
	int timeout = 1000;
	_setBuffer(msg, size, flag);

	bytes_sent = 0;
	int run = size;
	int sent_now = 0;

	while (bytes_sent < run) {			
		sent_now = ::send(client_socket, (const char*) &_buffer[bytes_sent], size, 0);

		if( sent_now > -1 ){
			size -= sent_now;
			bytes_sent += sent_now;
		}
		
		if (timeout) timeout--;
		else return -1;
	}

	return bytes_sent;     
}

